{
  "name": "pdf-template",
  "version": "1.0.0",
  "description": "Multi-tenant Resume PDF Generation SaaS",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "tsx watch src/server.ts",
    "lint": "eslint src/ sdk/",
    "lint:fix": "eslint src/ sdk/ --fix",
    "format": "prettier --write \"src/**/*.ts\" \"sdk/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"sdk/**/*.ts\"",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "pdf",
    "resume",
    "saas",
    "multi-tenant"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cloudinary": "^2.5.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "handlebars": "^4.7.8",
    "helmet": "^8.0.0",
    "inngest": "^3.27.6",
    "mongoose": "^8.10.1",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.16",
    "puppeteer": "^24.3.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.6",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.13.4",
    "@types/nodemailer": "^6.4.17",
    "@types/uuid": "^10.0.0",
    "eslint": "^9.20.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-import": "^2.31.0",
    "prettier": "^3.5.2",
    "tsx": "^4.19.3",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.1"
  }
}
